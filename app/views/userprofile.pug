doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    title User Detail
  body
    section.container
      for userDetail in result
        P #{userDetail.fname}
        P #{userDetail.lname}
        P #{userDetail.username}
        P #{userDetail.email}
        P #{userDetail.mobile}
        P #{userDetail.password}
        p
          a(href='http://localhost:3002/user/update/'+userDetail._id) Profile